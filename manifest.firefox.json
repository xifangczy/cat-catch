{
  "background": {
    "scripts": [
      "js/firefox.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "__MSG_catCatch__",
    "default_popup": "popup.html"
  },
  "description": "__MSG_description__",
  "icons": {
    "64": "img/icon.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2,
  "name": "__MSG_catCatch__",
  "homepage_url": "https://github.com/xifangczy/cat-catch",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "downloads",
    "storage",
    "webNavigation",
    "alarms",
    "scripting",
    "*://*/*",
    "<all_urls>",
    "declarativeNetRequest"
  ],
  "commands": {
    "_execute_browser_action": {},
    "enable": {
      "description": "__MSG_pause__ / __MSG_enable__"
    },
    "auto_down": {
      "description": "__MSG_autoDownload__"
    },
    "catch": {
      "description": "__MSG_cacheCapture__"
    },
    "m3u8": {
      "description": "__MSG_m3u8Parser__"
    },
    "clear": {
      "description": "__MSG_clear__"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "xifangczy@gmail.com",
      "strict_min_version": "113.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/content-script.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "catch-script/*"
  ],
  "default_locale": "en",
  "version": "2.5.1",
  "incognito": "split"
}